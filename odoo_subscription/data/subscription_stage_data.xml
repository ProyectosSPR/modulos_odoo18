<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="subscription_stage_draft" model="subscription.stage">
            <field name="name">Draft</field>
            <field name="sequence">5</field>
            <field name="category">draft</field>
        </record>
        <record id="subscription_stage_progress" model="subscription.stage">
            <field name="name">In Progress</field>
            <field name="sequence">10</field>
            <field name="category">progress</field>
        </record>
        <record id="subscription_stage_closed" model="subscription.stage">
            <field name="name">Closed</field>
            <field name="sequence">15</field>
            <field name="category">closed</field>
            <field name="is_fold" eval="True"/>
        </record>
    </data>
</odoo>
